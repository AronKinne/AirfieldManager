{
	"name": "Traktor",
	"type": "vehicle",
	"show": false,
	"img": "data/images/traktor.png",
	"speed": 0.2,
	"states": [
		"IN_KFZHALLE"
	],
	"menu": [
		{
			"name": "Rausfahren",
			"if": {
				"states": [
					"IN_KFZHALLE"
				],
				"object": {
					"path": "data/interactables/static_objects/vorfeld.json",
					"noStates": [
						"HAT_VEHICLE"
					]
				}
			},
			"then": {
				"remStates": [
					"IN_KFZHALLE"
				],
				"show": true,
				"pos": {
					"x": 1393,
					"y": 71,
					"w": 7,
					"h": 10,
					"d": 200
				}
			}
		},
		{
			"name": "In KFZ-Halle fahren",
			"if": {
				"noStates": [
					"IN_KFZHALLE",
					"HAT_FLUGZEUG"
				],
				"states": [
					"IN_APRON"
				]
			},
			"then": {
				"addStates": [
					"IN_KFZHALLE"
				],
				"show": false
			}
		},
		{
			"name": "Verbinden mit Flugzeug...",
			"if": {
				"noStates": [
					"IN_KFZHALLE",
					"HAT_FLUGZEUG"
				]
			},
			"then": {
				"function": "connectToPlane"
			}
		},
		{
			"name": "Seil entfernen",
			"if": {
				"states": [
					"HAT_FLUGZEUG"
				],
				"noStates": [
					"IN_HANGAR"
				]
			},
			"then": {
				"exec": "removeConnection",
				"remStates": [
					"HAT_FLUGZEUG"
				]
			}
		},
		{
			"name": "Fahre zu...",
			"if": {
				"noStates": [
					"IN_KFZHALLE"
				]
			},
			"then": {
				"function": "goTo"
			}
		}
	]
}